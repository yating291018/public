.moviecontainer {
  padding: 20rpx;
}
.movietitle {
  display: flex;
  justify-content: space-between;
  margin-bottom: 10rpx;
}
.movietitle .more {
  color: #24519F;
}
.scrollwrapper {
  display: flex;
  white-space: nowrap;
}
.movieitem{
  width: 200rpx;
  height: 240rpx;
  display: inline-block;
  margin-right: 40rpx;
}
.movieitem image {
  width: 100%;
  height: 100%;
}
